<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="OrangeHRM - UI automation test" verbose="1">
    <parameter name="webUrl" value="http://localhost:88/orangehrm"></parameter>

    <listeners>
        <listener class-name="core.MethodListener"></listener>
        <listener class-name="reportConfig.AllureTestListener"></listener>
    </listeners>

    <test name="Run on Firefox broser">
        <parameter name="browser" value="Firefox"></parameter>
        <classes>
            <class name="orangehrm.Login_11_Allure_Report"/>
        </classes>
    </test>

</suite>